<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <PackageIcon>elfbot-icon.png</PackageIcon>
    <ApplicationIcon>elfbot-icon.ico</ApplicationIcon>
	<DebugType>embedded</DebugType>
  </PropertyGroup>


	<ItemGroup>
    <None Remove="elfbot-icon-large.png" />
    <None Remove="elfbot-icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="elfbot-icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="elfbot-icon.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="InputSimulatorPlus" Version="1.0.7" />
    <PackageReference Include="Memory.dll.x64" Version="1.2.24" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="elfbot-icon-large.png" />
    <Resource Include="elfbot-icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Update="elfbot-icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
